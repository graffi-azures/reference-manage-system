<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="280.0" prefWidth="480.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.ImportController">
   <children>
      <VBox layoutX="6.0" layoutY="6.0" AnchorPane.bottomAnchor="5.0" AnchorPane.leftAnchor="5.0" AnchorPane.rightAnchor="5.0" AnchorPane.topAnchor="5.0">
         <children>
            <HBox prefHeight="50.0">
               <VBox.margin>
                  <Insets top="10.0" />
               </VBox.margin>
               <children>
                  <Label prefHeight="26.0" text="Import File:">
                     <HBox.margin>
                        <Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
                     </HBox.margin>
                  </Label>
                  <TextField fx:id="txtFiled" prefHeight="26.0">
                     <HBox.margin>
                        <Insets bottom="15.0" left="20.0" right="15.0" top="15.0" />
                     </HBox.margin>
                  </TextField>
                  <Button fx:id="btnchoose" mnemonicParsing="false" onMouseReleased="#onChooseClickAction" prefHeight="26.0" text="choose">
                     <HBox.margin>
                        <Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
                     </HBox.margin>
                  </Button>
               </children>
            </HBox>
            <HBox prefHeight="50.0">
               <children>
                  <Label prefHeight="26.0" text="Import Option:">
                     <HBox.margin>
                        <Insets bottom="15.0" left="15.0" right="10.0" top="15.0" />
                     </HBox.margin>
                  </Label>
                  <ComboBox fx:id="cbxIop" prefHeight="26.0" prefWidth="160.0">
                     <HBox.margin>
                        <Insets bottom="15.0" left="5.0" right="15.0" top="15.0" />
                     </HBox.margin>
                  </ComboBox>
               </children>
            </HBox>
            <HBox prefHeight="50.0">
               <children>
                  <Label prefHeight="26.0" text="Duplicates:">
                     <HBox.margin>
                        <Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
                     </HBox.margin>
                  </Label>
                  <ChoiceBox fx:id="cbxDup" prefHeight="24.0" prefWidth="160.0">
                     <HBox.margin>
                        <Insets bottom="15.0" left="23.0" right="15.0" top="15.0" />
                     </HBox.margin>
                  </ChoiceBox>
               </children></HBox>
            <HBox prefHeight="50.0">
               <children>
                  <Label prefHeight="26.0" text="Text Translation:">
                     <HBox.margin>
                        <Insets bottom="15.0" left="15.0" right="5.0" top="15.0" />
                     </HBox.margin>
                  </Label>
                  <ChoiceBox fx:id="cbxTsl" prefHeight="24.0" prefWidth="160.0">
                     <HBox.margin>
                        <Insets bottom="15.0" left="2.0" right="15.0" top="15.0" />
                     </HBox.margin>
                  </ChoiceBox>
               </children></HBox>
            <HBox nodeOrientation="RIGHT_TO_LEFT" prefHeight="50.0">
               <children>
                  <Button fx:id="btnCel" mnemonicParsing="false" onMouseReleased="#onCancleAction" text="Cancel">
                     <HBox.margin>
                        <Insets bottom="10.0" left="60.0" right="10.0" top="10.0" />
                     </HBox.margin>
                  </Button>
                  <Button fx:id="btnImp" mnemonicParsing="false" onMouseReleased="#onImportClickAction" text="Import">
                     <HBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </HBox.margin>
                  </Button>
               </children></HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
